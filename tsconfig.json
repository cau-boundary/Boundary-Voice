{
  "compilerOptions": {
    "target": "es5",
    "lib": [
      "dom",
      "dom.iterable",
      "esnext"
    ],
    "allowJs": true,
    "skipLibCheck": true,
    "strict": false,
    "forceConsistentCasingInFileNames": true,
    "noEmit": true,
    "esModuleInterop": true,
    "module": "esnext",
    "moduleResolution": "node",
    "resolveJsonModule": true,
    "isolatedModules": true,
    "jsx": "preserve",
    "baseUrl": ".",
    "paths": {
      "@src/*": ["./src/*"]
    }
  },
  "include": [
    "next-env.d.ts",
    "**/*.ts",
    "**/*.tsx"
, "src/hooks/useLocalStream.js", "src/hooks/useWebSocket.js", "src/hooks/useUserMedia.js", "src/hooks/useStream.ts", "src/hooks/useRemoteStream.js", "src/hooks/usePeer.js", "src/components/webRTC/localStream/localStream.js"  ],
  "exclude": [
    "node_modules"
  ]
}
